# Local Recipes
ex:Shopska_Salad
    a schema:Recipe ;
    schema:name "Shopska Salad" ;
    schema:recipeIngredient "Tomato", "Cucumber", "Pepper", "Cheese", "Olives" ;
    schema:recipeInstructions "All ingredients are chopped into small pieces and mixed. The cheese is grated on top and garnished with olives." .

ex:Kifli
    a schema:Recipe ;
    schema:name "Kifli" ;
    schema:recipeIngredient "Flour", "Milk", "Oil", "Yeast", "Salt", "Sugar" ;
    schema:recipeInstructions "Dough is made from all the ingredients and left to rise. Rolls are then formed and baked in a preheated oven at 180 degrees until golden." .

ex:Moussaka
    a schema:Recipe ;
    schema:name "Moussaka" ;
    schema:recipeIngredient "Potatoes", "Minced meat", "Onion", "Eggs", "Milk" ;
    schema:recipeInstructions "The potatoes are sliced into rounds and layered in a baking dish. Fried minced meat with onion is added, followed by another layer of potatoes. It is poured over with eggs and milk and baked in the oven." .

ex:Zelnik
    a schema:Recipe ;
    schema:name "Zelnik" ;
    schema:recipeIngredient "Flour", "Water", "Salt", "Oil", "Spinach", "Cheese" ;
    schema:recipeInstructions "Dough is made from flour, water, and salt. Sheets of dough are rolled out, filled with spinach and cheese, and layered in a baking dish. It is baked in the oven." .
