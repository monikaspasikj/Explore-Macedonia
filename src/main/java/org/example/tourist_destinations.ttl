@prefix schema: <http://schema.org/> .
@prefix geo: <http://www.geonames.org/ontology#> .
@prefix ex: <http://example.org/tourist_destinations/> .

# Охрид
ex:Охрид
    a schema:City ;
    schema:name "Охрид" ;
    schema:containsPlace ex:Охридско_Езеро, ex:Самоилова_Тврдина, ex:Црква_Свети_Јован_Канео, ex:Музеј_на_Охрид .

ex:Охридско_Езеро
    a schema:TouristAttraction ;
    schema:name "Охридско Езеро" ;
    schema:description "Едно од најстарите и најдлабоките езера во Европа, познато по својата кристално чиста вода." ;
    schema:containedInPlace ex:Охрид .

ex:Самоилова_Тврдина
    a schema:TouristAttraction ;
    schema:name "Самоилова Тврдина" ;
    schema:description "Средновековна тврдина која датира од времето на Цар Самоил." ;
    schema:containedInPlace ex:Охрид .

ex:Црква_Свети_Јован_Канео
    a schema:TouristAttraction ;
    schema:name "Црква Свети Јован Канео" ;
    schema:description "Позната црква лоцирана на карпа над Охридското Езеро, со прекрасен поглед кон езерото." ;
    schema:containedInPlace ex:Охрид .

ex:Музеј_на_Охрид
    a schema:Museum ;
    schema:name "Музеј на Охрид" ;
    schema:description "Музеј кој ги прикажува богатата историја и култура на Охрид и околината." ;
    schema:containedInPlace ex:Охрид .

# Скопје
ex:Скопје
    a schema:City ;
    schema:name "Скопје" ;
    schema:containsPlace ex:Кале, ex:Камен_Мост, ex:Музеј_на_Македонија, ex:Парк_на_Природата_Водно .

ex:Кале
    a schema:TouristAttraction ;
    schema:name "Кале" ;
    schema:description "Средновековна тврдина со поглед кон центарот на Скопје." ;
    schema:containedInPlace ex:Скопје .

ex:Камен_Мост
    a schema:TouristAttraction ;
    schema:name "Камен Мост" ;
    schema:description "Мост од османлискиот период кој ги поврзува двата дела на градот." ;
    schema:containedInPlace ex:Скопје .

ex:Музеј_на_Македонија
    a schema:Museum ;
    schema:name "Музеј на Македонија" ;
    schema:description "Национален музеј кој ги прикажува историјата, културата и уметноста на Македонија." ;
    schema:containedInPlace ex:Скопје .

ex:Парк_на_Природата_Водно
    a schema:TouristAttraction ;
    schema:name "Парк на Природата Водно" ;
    schema:description "Популарно место за рекреација и планинарење, со крстот Милениум на врвот." ;
    schema:containedInPlace ex:Скопје .

# Битола
ex:Битола
    a schema:City ;
    schema:name "Битола" ;
    schema:containsPlace ex:Хераклеја_Линкестис, ex:Широк_Сокак, ex:Музеј_на_Битола, ex:Пелистер .

ex:Хераклеја_Линкестис
    a schema:TouristAttraction ;
    schema:name "Хераклеја Линкестис" ;
    schema:description "Антички град основан од Филип II Македонски, со значајни археолошки остатоци." ;
    schema:containedInPlace ex:Битола .

ex:Широк_Сокак
    a schema:TouristAttraction ;
    schema:name "Широк Сокак" ;
    schema:description "Главна пешачка улица со многу кафулиња, ресторани и историски згради." ;
    schema:containedInPlace ex:Битола .

ex:Музеј_на_Битола
    a schema:Museum ;
    schema:name "Музеј на Битола" ;
    schema:description "Музеј кој ги документира историјата и културата на Битола и нејзината околина." ;
    schema:containedInPlace ex:Битола .

ex:Пелистер
    a schema:TouristAttraction ;
    schema:name "Пелистер" ;
    schema:description "Национален парк познат по својата биолошка разновидност и убави пејзажи." ;
    schema:containedInPlace ex:Битола .

