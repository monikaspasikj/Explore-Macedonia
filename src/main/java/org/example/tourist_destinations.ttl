@prefix schema: <http://schema.org/> .
@prefix geo: <http://www.geonames.org/ontology#> .
@prefix ex: <http://example.org/tourist_destinations/> .

# Ohrid
ex:Ohrid
    a schema:City ;
    schema:name "Ohrid" ;
    schema:containsPlace ex:Ohrid_Lake, ex:Samuil_Fortress, ex:Church_of_St_John_Kaneo, ex:Museum_of_Ohrid .

ex:Ohrid_Lake
    a schema:TouristAttraction ;
    schema:name "Ohrid Lake" ;
    schema:description "One of the oldest and deepest lakes in Europe, known for its crystal clear water." ;
    schema:containedInPlace ex:Ohrid .

ex:Samuil_Fortress
    a schema:TouristAttraction ;
    schema:name "Samuil Fortress" ;
    schema:description "A medieval fortress dating back to the time of Tsar Samuil." ;
    schema:containedInPlace ex:Ohrid .

ex:Church_of_St_John_Kaneo
    a schema:TouristAttraction ;
    schema:name "Church of St. John at Kaneo" ;
    schema:description "A famous church located on a cliff above Ohrid Lake, offering a stunning view of the lake." ;
    schema:containedInPlace ex:Ohrid .

ex:Museum_of_Ohrid
    a schema:Museum ;
    schema:name "Museum of Ohrid" ;
    schema:description "A museum showcasing the rich history and culture of Ohrid and its surroundings." ;
    schema:containedInPlace ex:Ohrid .

# Skopje
ex:Skopje
    a schema:City ;
    schema:name "Skopje" ;
    schema:containsPlace ex:Kale, ex:Stone_Bridge, ex:Museum_of_Macedonia, ex:Vodno_Nature_Park .

ex:Kale
    a schema:TouristAttraction ;
    schema:name "Kale" ;
    schema:description "A medieval fortress overlooking the center of Skopje." ;
    schema:containedInPlace ex:Skopje .

ex:Stone_Bridge
    a schema:TouristAttraction ;
    schema:name "Stone Bridge" ;
    schema:description "A bridge from the Ottoman period connecting both sides of the city." ;
    schema:containedInPlace ex:Skopje .

ex:Museum_of_Macedonia
    a schema:Museum ;
    schema:name "Museum of Macedonia" ;
    schema:description "The national museum that exhibits the history, culture, and art of Macedonia." ;
    schema:containedInPlace ex:Skopje .

ex:Vodno_Nature_Park
    a schema:TouristAttraction ;
    schema:name "Vodno Nature Park" ;
    schema:description "A popular spot for recreation and hiking, featuring the Millennium Cross at the top." ;
    schema:containedInPlace ex:Skopje .

